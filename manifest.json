{
  "name": "MM Tags",
  "version": "1.3",
  "description": "This extension lets load a list of urls and analyze their tags.",
  "permissions": ["*://*.serving-sys.com/*", "tabs", "webRequest", "webRequestBlocking", "debugger"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Set URLs",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "120": "images/icon120.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "120": "images/icon120.png"
  },
  "manifest_version": 2
}
